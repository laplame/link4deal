{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "node server/server.js",
    "server:dev": "nodemon server/server.js",
    "server:start": "node server/server.js",
    "python:install": "cd server_py && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "python:dev": "cd server_py && source venv/bin/activate && python main.py",
    "python:start": "cd server_py && source venv/bin/activate && python main.py",
    "start:all": "concurrently \"npm run server:dev\" \"npm run python:dev\" \"npm run dev\"",
    "db:start": "mongod --dbpath ./data/db",
    "db:stop": "pkill mongod",
    "setup": "npm install && npm run python:install && mkdir -p data/db public/uploads logs",
    "verify-api": "node scripts/verify-api.js"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "lucide-react": "^0.540.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "nodemon": "^3.0.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
